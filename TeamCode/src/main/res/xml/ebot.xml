<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <LynxUsbDevice name="Control Hub Portal" serialNumber="(embedded)" parentModuleAddress="173">
        <LynxModule name="Control Hub" port="173">
            <!-- Drivetrain -->
            <goBILDA5202SeriesMotor name="leftFront" port="3" />
            <goBILDA5202SeriesMotor name="rightFront" port="2" />

            <!-- Outtake -->
            <goBILDA5202SeriesMotor name="ferrisWheelMotor" port="1" />
            <Matrix12vMotor name="rightFlywheel" port="0" />
            <Servo name="rightKicker" port="5" />
            <ContinuousRotationServo name="rightAngleServo" port="4" />

            <!-- I2C -->
            <goBILDAPinpoint name="pinpoint" port="1" bus="0" />
            <RevColorSensorV3 name="rightColorSensor" port="0" bus="1" />

            <!-- LED -->
            <Servo name="rightIndicatorLED" port="2" />

            <ControlHubImuBHI260AP name="imu" port="0" bus="0" />
        </LynxModule>
        <LynxModule name="Expansion Hub 2" port="2">
            <!-- Drivetrain -->
            <goBILDA5202SeriesMotor name="leftBack" port="2" />
            <goBILDA5202SeriesMotor name="rightBack" port="3" />

            <!-- Intake -->
            <goBILDA5202SeriesMotor name="intakeMotor" port="1" />

            <!-- Outtake -->
            <Matrix12vMotor name="leftFlywheel" port="0" />
            <Servo name="leftKicker" port="1" />
            <ContinuousRotationServo name="leftAngleServo" port="0" />

            <!-- I2C -->
            <RevColorSensorV3 name="leftColorSensor" port="0" bus="0" />
            <RevColorSensorV3 name="lowerColorSensor" port="0" bus="2" />

            <!-- LED -->
            <Servo name="leftIndicatorLED" port="2" />
        </LynxModule>
    </LynxUsbDevice>
    <EthernetDevice name="limelight" serialNumber="EthernetOverUsb:eth0:172.29.0.25" port="0" ipAddress="172.29.0.1" />
</Robot>
