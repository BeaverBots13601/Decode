<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <LynxUsbDevice name="Control Hub Portal" serialNumber="(embedded)" parentModuleAddress="173">
        <LynxModule name="Control Hub" port="173">
            <!-- Drivetrain -->
            <goBILDA5202SeriesMotor name="leftFront" port="3" />
            <goBILDA5202SeriesMotor name="rightFront" port="2" />
            <goBILDA5202SeriesMotor name="leftBack" port="0" />
            <goBILDA5202SeriesMotor name="rightBack" port="1" />

            <!-- Outtake -->
            <ContinuousRotationServo name="turntableAxon" port="5" />
            <Servo name="locker" port="2" />
            <Servo name="leftKickerServo" port="4" />

            <!-- Axon Encoders -->
            <AnalogInput name="turntableEncoder" port="0" />

            <!-- LED -->
            <Servo name="leftIndicatorLED" port="3" />

            <!-- I2C -->
            <RevColorSensorV3 name="leftLiftColorSensor" port="0" bus="0" />

            <!-- Built-in IMU -->
            <ControlHubImuBHI260AP name="imu" port="0" bus="0" />
        </LynxModule>
        <LynxModule name="Expansion Hub 2" port="2">
            <!-- Intake -->
            <goBILDA5202SeriesMotor name="intakeMotor" port="0" />

            <!-- Outtake -->
            <Servo name="rightKickerServo" port="5" />
            <Matrix12vMotor name="flywheel" port="1" />

            <!-- I2C -->
            <RevColorSensorV3 name="intakeColorSensor" port="0" bus="2" />
            <RevColorSensorV3 name="rightLiftColorSensor" port="0" bus="1" />
            <goBILDAPinpoint name="pinpoint" port="0" bus="0" />

            <!-- Risers -->
            <goBILDA5202SeriesMotor name="leftRiser" port="3" />
            <goBILDA5202SeriesMotor name="rightRiser" port="2" />

            <!-- LED -->
            <Servo name="rightIndicatorLED" port="1" />
        </LynxModule>
    </LynxUsbDevice>
    <EthernetDevice name="limelight" serialNumber="EthernetOverUsb:eth0:172.29.0.25" port="0" ipAddress="172.29.0.1" />
</Robot>
